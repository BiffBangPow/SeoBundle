<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="cmf_seo.persistence.phpcr.listener">Symfony\Cmf\Bundle\SeoBundle\Doctrine\Phpcr\SeoMetadataListener</parameter>

    </parameters>

    <services>
        <service id="cmf_seo.persistence.phpcr.listener" class="%cmf_seo.persistence.phpcr.listener%">
            <tag name="doctrine_phpcr.event_subscriber" />
        </service>
    </services>

</container>
